{
  "name": "vue-webdev",
  "version": "1.0.0",
  "description": "Webdevelop",
  "author": "webdevelop.pro",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --inline --host 0.0.0.0 --hot --mode development --hot-styles",
    "start:prod": "NODE_ENV=production webpack-dev-server --host 0.0.0.0 --mode production",
    "start:analyze": "NODE_ENV=development webpack-dev-server --inline --host 0.0.0.0 --hot --mode development --analyze",
    "build:dev": "rm -rf dist/* && NODE_ENV=development webpack --mode development --env.legacy=true",
    "build": "rm -rf dist/* && NODE_ENV=production webpack --mode production --env.legacy=true",
    "build:analyze": "rm -rf dist/* && NODE_ENV=production webpack --mode production --analyze --profile --json > compilation-stats.json --env.legacy=true",
    "lint": "NODE_ENV=production eslint --ext .js,.vue src test/unit",
    "lint:fix": "NODE_ENV=production eslint --ext .js,.vue src test/unit --fix",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "autoprefixer": "^9.3.1",
    "babel-eslint": "^9.0.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.1",
    "lodash.set": "^4.3.2",
    "lodash.startcase": "^4.4.0",
    "lodash.upperfirst": "^4.3.1",
    "owl-carousel": "^1.0.0",
    "poststylus": "^1.0.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "style-loader": "^0.23.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue": "^2.5.17",
    "vue-loader": "^15.4.1",
    "vue-media": "^1.1.1",
    "vue-mq": "^1.0.1",
    "vue-owl-carousel": "^2.0.3",
    "vue-router": "^3.0.1",
    "vue-slick": "^1.1.15",
    "vue-template-compiler": "^2.5.17",
    "vuetify": "^1.3.12",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.5.4",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "yarn": "^1.15.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.2",
    "css-loader": "^2.1.1",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-vue": "^4.7.1",
    "husky": "^0.14.3",
    "jquery": "^3.4.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "path": "^0.12.7",
    "pug-lint-vue": "^0.1.3",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "vue-pug-lint-loader": "^0.0.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-merge": "^4.1.5"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "not ie <= 8"
  ]
}
